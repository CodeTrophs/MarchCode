   ],
   "source": [
    "number = int(input(\"Enter the number\\n\"))\n",
    "if number % 5 == 0 and number % 11 == 0:\n",
    "    print(\"The number is divisible by both 5 and 11\")\n",
    "else:\n",
    "    print(\"The number is not divisible by both 5 and 11\")"
   ]
